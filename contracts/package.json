{
  "name": "private-dao-contracts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "compile": "~/.aztec/bin/aztec compile",
    "compile:package": "~/.aztec/bin/aztec compile --package",
    "compile:registry": "~/.aztec/bin/aztec compile --package cloak_registry",
    "compile:memberships": "~/.aztec/bin/aztec compile --package cloak_memberships",
    "deploy:memberships": "yarn ts-node scripts/deploy_memberships.ts",
    "copy-artifacts": "cp target/cloak_memberships-CloakMemberships.json target/cloak_registry-CloakRegistry.json ../app/src/lib/aztec/artifacts/ 2>/dev/null; echo 'Artifacts copied to app'",
    "test": "nargo test",
    "deploy:local": "yarn ts-node scripts/deploy_dao.ts --network local",
    "deploy:devnet": "yarn ts-node scripts/deploy_dao.ts --network devnet",
    "deploy:fpc": "yarn ts-node scripts/deploy_fpc.ts",
    "deploy:registry": "yarn ts-node scripts/deploy_registry.ts",
    "deploy:connections": "yarn ts-node scripts/deploy_connections.ts",
    "deploy:starred": "yarn ts-node scripts/deploy_starred_cloaks.ts"
  },
  "dependencies": {
    "@aztec/aztec.js": "3.0.0-devnet.6-patch.1",
    "@aztec/accounts": "3.0.0-devnet.6-patch.1",
    "@aztec/noir-contracts.js": "3.0.0-devnet.6-patch.1",
    "@aztec/stdlib": "3.0.0-devnet.6-patch.1",
    "@aztec/test-wallet": "3.0.0-devnet.6-patch.1",
    "@aztec/pxe": "3.0.0-devnet.6-patch.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
